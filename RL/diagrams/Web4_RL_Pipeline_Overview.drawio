                </object>
                <mxCell id="arrow-decision1-pass" value="âœ… PASS" style="endArrow=classic;html=1;rounded=0;strokeWidth=3;strokeColor=#2E7D32;exitX=0.5;exitY=1;exitDx=0;exitDy=0;entryX=0.5;entryY=0;entryDx=0;entryDy=0;fontSize=11;fontStyle=1;fontColor=#2E7D32;" parent="1" source="decision1" target="stage2-box" edge="1">
                    <mxGeometry width="50" height="50" relative="1" as="geometry">
                        <mxPoint x="800" y="1050" as="sourcePoint"/>
