                <mxCell id="arrow-decision2-optional" value="âœ… PASS&#xa;(Optional)" style="endArrow=classic;html=1;rounded=0;strokeWidth=3;strokeColor=#D84315;exitX=0.5;exitY=1;exitDx=0;exitDy=0;entryX=0.5;entryY=0;entryDx=0;entryDy=0;fontSize=11;fontStyle=1;fontColor=#D84315;dashed=1;" parent="1" source="decision2" target="stage3-box" edge="1">
