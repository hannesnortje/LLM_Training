                <mxCell id="success_title" value="âœ… Success Criteria by Phase" style="text;html=1;strokeColor=none;fillColor=#4CAF50;align=center;verticalAlign=middle;whiteSpace=wrap;rounded=1;fontSize=14;fontStyle=1;fontColor=#FFFFFF" parent="1" vertex="1">
